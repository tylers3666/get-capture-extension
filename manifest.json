{
  "name": "Login Form GET Request Capturer",
  "version": "1.0",
  "manifest_version": 3,
  "description": "Captures and displays GET request URLs upon entering login forms.",
  "permissions": [
    "webRequest",
    "activeTab",
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
